{
  "presets": [
    [
      "env", {
        "targets": {
          "browsers": ["last 4 versions", "not ie < 8"]
        },
        "loose": false
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime", {
        "helpers": true,
        "polyfill": true,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    "transform-decorators-legacy"
  ]
}